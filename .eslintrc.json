
{
  "extends": "vaadin",
  "parserOptions": {
    "ecmaVersion": 6
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "plugins": [
    "html"
  ],
  "rules": {
    "brace-style": "off",
    "no-var": "off",
    "require-jsdoc": "off"
  },
  "globals": {
    "CustomElements": false,
    "HTMLImports": false,
    "Polymer": true,
    "WeakMap": false,
    "Vaadin": false,
    "Alump": false,
    "DBarElement": false
  }
}
